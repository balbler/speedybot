{
    "name": "speedybot",
    "version": "0.0.2",
    "description": "Speedy & easy way to rapidly iterate with conversation bots",
    "bin": {
        "speedybot": "./dist/src/cli/index.js"
    },
    "scripts": {
        "patch": "npx np patch",
        "prepublish": "npm run build",
        "tunnel": "node dist/src/cli/index.js tunnel 8000",
        "start:dist": "node dist/src/server",
        "start": "node -r ts-node/register ./src/server/index.ts",
        "start:inspect": "node --inspect=5858 -r ts-node/register ./src/server/index.ts",
        "dev": "nodemon",
        "build": "tsc",
        "setup": "npm i && tsc"
    },
    "keywords": [
        "bot",
        "scaffold",
        "demo",
        "speedybot"
    ],
    "author": "valgaze@gmail.com",
    "license": "MIT",
    "dependencies": {
        "@types/express": "^4.17.13",
        "chalk": "^4.1.2",
        "express": "^4.17.1",
        "fs-extra": "^10.0.0",
        "ngrok": "^3.2.7",
        "webex-node-bot-framework": "^2.3.7",
        "@types/node": "^13.13.52"
    },
    "files": [
        "dist/**"
    ],
    "devDependencies": {
        "ts-node": "^8.10.1",
        "nodemon": "^2.0.4",
        "typescript": "^3.8.3"
    }
}
